@model ProductsListViewModel

<table>
    <thead>
        <tr>
            <th>Id</th>
            <th>Name</th>
            <th>Desc</th>
            <th>Price</th>
            <th>Stock</th>
        </tr>
    </thead>
    @foreach (var product in Model.Products)
    {
    <tr>
        <td>@product.Id</td>
        <td>@product.Name</td>
        <td>@product.Description</td>
        <td>@product.Price</td>
        <td>@product.Stock</td>
    </tr>
    }
</table>
